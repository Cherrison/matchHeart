<view class='UCenter-bg'>
  <image src='{{usrinfo.avatarUrl}}' class='round png' mode='widthFix'></image>
  <view class='text-xl'>{{usrinfo.nickName}}
  </view>
  <view class='margin-top-sm'>
    <text class='text-bold;'>By:Psyche</text>
  </view>
  <image src='https://image.weilanwl.com/gif/wave.gif' mode='scaleToFill' class='gif-wave'></image>
</view>
<button bindtap='toid'>{{idstatus==true?"身份已认证":"身份未认证"}}</button>

<view class="cu-list menu card-menu margin-top-xl margin-bottom-xl shadow-lg">
  <view class="cu-item arrow">
    <view class='content' bindtap='CopyLink' data-link='https://github.com'>
      <text class='icon-github text-grey'></text>
      <text class='text-grey'>GitHub</text>
    </view>
  </view>
  <view class="cu-item arrow">
    <navigator class='content' url='../about/about' hover-class='none'>
      <image src='/images/logo.png' class='png' mode='aspectFit'></image>
      <text class='text-grey'>关于心海</text>
    </navigator>
  </view>
  <view class="cu-item arrow">
    <navigator class='content' url='../log/log' hover-class='none'>
      <text class='icon-formfill text-green'></text>
      <text class='text-grey'>日志</text>
    </navigator>
  </view>
  <view class="cu-item arrow">
    <view class='content' bindtap="tomyExpert">
      <text class='icon-newsfill text-red'></text>
      <text class='text-grey'>我的预约</text>
    </view>
  </view>
  <view class="cu-item arrow">
    <button class='cu-btn content' open-type='feedback'>
      <text class='icon-writefill text-cyan'></text>
      <text class='text-grey'>意见反馈</text>
    </button>
  </view>
</view>

<view class='cu-tabbar-height'></view>