}
.center{
  display: flex;
  justify-content: center;
  align-content: center;
  align-items: center;
}
.result{
  width: 85%;
  border: 2px rgb(117, 117, 207)  solid;
  border-radius: 20rpx;
}
.containere {
  position: absolute;
  top: 150rpx;
  bottom: 0rpx;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: white;
} 

.item{
  box-shadow:grey 5rpx 5rpx 4rpx 4rpx inset;
  padding: 15rpx;
}
